{
    "name": "roelofjan-elsinga/aloia-cms-gui",
    "description": "The GUI for the roelofjan-elsinga/aloia-cms package",
    "keywords": [
        "laravel",
        "cms",
        "file",
        "user interface"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roelof Jan Elsinga",
            "email": "roelofjanelsinga@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AloiaCms\\GUI\\": "src/",
            "AloiaCms\\GUI\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "AloiaCms\\GUI\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/view": "^8.0|^9.0",
        "illuminate/http": "^8.0|^9.0",
        "genealabs/laravel-caffeine": "^8.0|^9.0",
        "roelofjan-elsinga/aloia-cms": "^4.0",
        "roelofjan-elsinga/aloia-cms-publish": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mikey179/vfsstream": "^1.6",
        "orchestra/testbench": "^7.4",
        "friendsofphp/php-cs-fixer": "^3.8"
    }
}
